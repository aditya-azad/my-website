---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - blog
    - tagList
  addAllPagesToCollections: true
layout: layouts/home.njk
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /tags/{{ tag }}/
---
<h1 class="main-page-heading">Tagged “{{ tag }}”</h1>

{% set displayTags = true %}
{% set postslist = collections[ tag ] %}
{% include "postslist.njk" %}
